{
  "name": "vue-lite-js",
  "version": "1.5.2",
  "description": "Clones Vue.js to implement a basic MVVM framework using TypeScript",
  "main": "dist/bundle.js",
  "types": "./dist/index.d.ts",
  "module": "dist/bundle.esm.js",
  "jsdelivr": "dist/bundle.min.js",
  "unpkg": "dist/bundle.min.js",
  "author": "heonys",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Heonys/vuelite/issues"
  },
  "homepage": "https://github.com/Heonys/vuelite#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Heonys/vuelite.git"
  },
  "keywords": [
    "vuelite",
    "vuejs",
    "mvvm"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run dev",
    "build": "rollup --config build/rollup.config.js",
    "build:dts": "dts-bundle-generator -o dist/index.d.ts src/**/*.ts",
    "dev": "webpack-dev-server --progress --config build/webpack.config.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.3.3",
    "rollup": "^3.29.4",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1"
  }
}
